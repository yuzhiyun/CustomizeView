<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yuzhiyun.customizeview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.RainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.activity.TitleViewActivity"
            android:label="@string/title_activity_title_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.activity.LearnOnMeasureActivity"
            android:label="@string/title_activity_learn_on_measure"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.activity.LearnGroupViewctivity"
            android:label="@string/title_activity_learn_group_viewctivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".view.activity.FlowLayoutActivity"></activity>
    </application>

</manifest>
